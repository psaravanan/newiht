#supersized-loader
{
    position: absolute;
    top: 50%;
    left: 50%;
    z-index: 0;
    width: 60px;
    height: 60px;
    margin: -30px 0 0 -30px;
    text-indent: -999em;
    /*background: url(../Images/HomeGallery/progress.gif) no-repeat center center;*/
}


/*Lockest Code Start*/
#gallery_contentarea
{
    display: block;
    text-decoration: none;
    float: right;
    margin-right: -10%;
    right: 0px;
    /*opacity: 0.85;
    margin-left: 42%;*/
    position: absolute;
    margin-top: -200px;
    top: 200px;
    width: 950px;
    height: 100%;
    z-index: -2;
    color: #fff;
    -webkit-transition: background-color 0.5s ease-out;
    -moz-transition: background-color 0.5s ease-out;
    -o-transition: background-color 0.5s ease-out;
    transition: background-color 0.5s ease-out;
    -ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorstr=#CC1F98D9,endColorstr=#CC1F98D9)"; /* IE8 */
    transform: skew(-32deg);
    -o-transform: skew(-32deg);
    -moz-transform: skew(-32deg);
    -webkit-transform: skew(-32deg);
}


