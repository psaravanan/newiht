/*Lockest Code close*/

.SlideObject
{
    position: absolute;
    left: -470px;
    top: 408px;
    transform: skew(32deg);
    -o-transform: skew(32deg);
    -moz-transform: skew(32deg);
    -webkit-transform: skew(32deg);
}

#supersized-Content
{
    opacity: 0.85;
    color: white;
    position: absolute;
    bottom: 20px;
    left: 458px;
    z-index: -1;
    width: 1125px;
    height: 595px;
    margin: -30px 0 0 -30px;
    /*text-indent: -999em;*/
    /*background-color: #0090de;*/
    transform: skew(32deg);
    -ms-transform: skew(32deg); /* IE 9 */
    -webkit-transform: skew(32deg); /* Safari and Chrome */
}

    #supersized-Content img, #supersized-Content h2
    {
        transform: skew(32deg);
        -ms-transform: skew(32deg); /* IE 9 */
        -webkit-transform: skew(32deg); /* Safari and Chrome */
    }

    #supersized-Content img, #supersized-Content h2
    {
        margin-top: 22%;
        margin-left: 15%;
    }



    #supersized-Content .supersized-Content-Objest
    {
        margin-top: 10%;
        margin-left: 9%;
        opacity: 1;
    }

#supersized
{
    display: block;
    position: fixed;
    left: 0;
    top: 0;
    overflow: hidden;
    z-index: -999;
    height: 100%;
    width: 100%;
}

    #supersized img
    {
        width: auto;
        height: auto;
        position: relative;
        display: none;
        outline: none;
        border: none;
    }

    #supersized.speed img
    {
        -ms-interpolation-mode: nearest-neighbor;
        image-rendering: -moz-crisp-edges;
    }
    /*Speed*/
    #supersized.quality img
    {
        -ms-interpolation-mode: bicubic;
        image-rendering: optimizeQuality;
    }
    /*Quality*/

    #supersized li
    {
        display: block;
        list-style: none;
        z-index: -30;
        position: fixed;
        overflow: hidden;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        background: #111;
    }

    #supersized a
    {
        width: 100%;
        height: 100%;
        display: block;
    }

    #supersized li.prevslide
    {
        z-index: -20;
    }

    #supersized li.activeslide
    {
        z-index: -10;
    }

    /*#supersized li.image-loading
    {
        background: #111 url(../Images/HomeGallery/progress.gif) no-repeat center center;
        width: 100%;
        height: 100%;
    }*/

    #supersized li.image-loading img
    {
        visibility: hidden;
    }

    #supersized li.prevslide img, #supersized li.activeslide img
    {
        display: inline;
    }

