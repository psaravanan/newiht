<script type="text/javascript">
    $(document).ready(function ($) {
        $('.cycle').cyclotron();
        $("#parentslide").mCustomScrollbar({

            horizontalScroll: true,
            theme: "light",
            autoHideScrollbar: false,
            scrollButtons: {
                enable: true
            }
        });

        $('.bg').fadeIn(400);
        $(".mCSB_container").animate({ left: "-578px" }, 600);
        $(".mCSB_dragger").animate({ left: "299px" }, 600);
    });
    $(window).load(function () {
        $('.Contactbg').fadeIn(400);
    });
    var map;
    if ($("#ContactMap").length > 0) {

        loadMap(40.74727, -73.98006, 'North America', 'AddNAmerica');
        //if ($("#ContactMap").length > 0) {
        //    alert('same');
        //    $('.mainrightPanel').animate({ 'z-index': "-5" });
        //}
        //else {
        //    alert('other');
        //    $('.mainrightPanel').animate({ 'z-index': "3" });
        //}

    }
    function loadMap(Lati, Long, locat, div) {

        $(".mCSB_container").css("left", "0px");
        $(".mCSB_dragger").css("left", "0px");

        $('.locationDetails h2').css({ color: "#000000" });
        $('.locationDetails h2 img').remove();
        $('#' + div + ' h2').css({ color: "#1F98D9" });
        // $('#' + div + ' h2 img').css({ display: 'block' });
        $('#' + div + ' h2').prepend('<img src="Images/' + div + '.png" />');
        
        $(".mCSB_container").animate({ left: "-578px" }, 900);
        $(".mCSB_dragger").animate({ left: "299px" }, 900);
        var map = new GMaps({
            el: '#ContactMap',
            lat: Lati,
            lng: Long,
            zoom: 14,
            zoomControl: true,
            zoomControlOpt: {
                style: 'SMALL',
                position: 'TOP_LEFT'
            },
            panControl: false,
        });

        map.addMarker({
            lat: Lati,
            lng: Long,
            title: locat,
            infoWindow: {
                content: '<p>' + locat + '</p>'
            }
        });


        //var map = new GMaps({
        //    el: '#ContactMap',
        //    lat: 51.5073346,
        //    lng: -0.1276831,
        //});
    }
</script>
